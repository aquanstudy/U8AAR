<?xml version="1.0" encoding="UTF-8"?>
<config>
    
    <params>
        <param name="MZW_Orientation" required="1" showName="横屏或者竖屏(landscape|portrait)" desc="拇指玩" bWriteInManifest="0" bWriteInClient="1" />
        <param name="TALKING_APPID" required="1" showName="talkingdata的appid" desc="meizu" bWriteInManifest="0" bWriteInClient="1" />
       	<param name="TALKING_CHANNEL" required="1" showName="talkingdata的渠道号" desc="meizu" bWriteInManifest="0" bWriteInClient="1" />     
    </params>
    
    <operations>
        <operation step="1" type="mergeManifest" from="SDKManifest.xml" to="AndroidManifest.xml" />
        <operation step="2" type="copyRes" from="assets" to="assets" />
        <operation step="3" type="copyRes" from="libs" to="lib" />
        <operation step="4" type="copyRes" from="res" to="res" />
        <operation step="5" type="copyRes" from="root" to=""/>
    </operations>
    
    <plugins>
        <plugin name="com.u8.sdk.MZWUser" type="1" desc="用户登录接口"/>
        <plugin name="com.u8.sdk.MZWPay" type="2" desc="用户支付接口"/>
    </plugins>
    
    <version>
        <name>拇指玩</name>
        <versionCode>3</versionCode>
        <versionName>3.6.1.6</versionName>
    </version>    
    
</config>