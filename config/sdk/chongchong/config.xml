<?xml version="1.0" encoding="UTF-8"?>
<config>
    
    <params>
        <param name="app_id" required="1" showName="AppID" desc="虫虫游戏的apppid" bWriteInManifest="1" bWriteInClient="0" />
        <param name="developer_key" required="1" showName="developerKey" desc="虫虫游戏的developerKey" bWriteInManifest="1" bWriteInClient="0" /> 
        <param name="TALKING_APPID" required="1" showName="talkingdata的appid" desc="360" bWriteInManifest="0" bWriteInClient="1" />
       	<param name="TALKING_CHANNEL" required="1" showName="talkingdata的渠道号" desc="360" bWriteInManifest="0" bWriteInClient="1" />
    </params>
    
    <operations>
        <operation step="1" type="mergeManifest" from="SDKManifest.xml" to="AndroidManifest.xml" />
        <operation step="2" type="copyRes" from="assets" to="assets" />
        <operation step="3" type="copyRes" from="libs" to="lib" />
        <operation step="4" type="copyRes" from="res" to="res" />
    </operations>
    
    <plugins>
        <plugin name="com.u8.sdk.LionUser" type="1" desc="用户登录接口"/>
        <plugin name="com.u8.sdk.LionPay" type="2" desc="用户支付接口"/>
    </plugins>
    
    <version>
        <name>虫虫游戏</name>
        <versionCode>1</versionCode>
        <versionName>1.7.7</versionName>
    </version>     
    
</config>