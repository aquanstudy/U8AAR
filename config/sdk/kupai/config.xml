<?xml version="1.0" encoding="UTF-8"?>
<config>
    
    <params>
        <param name="CoolPad_AppID" required="1" showName="AppID" desc="酷派" bWriteInManifest="0" bWriteInClient="1" />
        <param name="CoolPad_AppKey" required="1" showName="AppID" desc="酷派" bWriteInManifest="0" bWriteInClient="1" />
        <param name="CoolPad_PayKey" required="1" showName="AppID" desc="酷派" bWriteInManifest="0" bWriteInClient="1" />
        <param name="CoolPad_Orientation" required="1" showName="横竖屏(landscape|portrait)" desc="酷派" bWriteInManifest="0" bWriteInClient="1" />
        <param name="TALKING_APPID" required="1" showName="talkingdata的appid" desc="分配给酷派的渠道appid" bWriteInManifest="0" bWriteInClient="1" />
       	<param name="TALKING_CHANNEL" required="1" showName="talkingdata的渠道号" desc="分配给酷派的渠道号" bWriteInManifest="0" bWriteInClient="1" />
    </params>
    
    <operations>
        <operation step="1" type="mergeManifest" from="SDKManifest.xml" to="AndroidManifest.xml" />
        <operation step="2" type="copyRes" from="assets" to="assets" />
        <operation step="3" type="copyRes" from="libs" to="lib" />
        <operation step="4" type="copyRes" from="res" to="res" />
        <operation step="5" type="copyRes" from="root" to="" />
    </operations>
    
    <plugins>
        <plugin name="com.u8.sdk.CoolPadUser" type="1" desc="用户登录接口"/>
        <plugin name="com.u8.sdk.CoolPadPay" type="2" desc="用户支付接口"/>
    </plugins>
    
    <version>
        <name>酷派</name>
        <versionCode>1</versionCode>
        <versionName>3.x.x</versionName>
    </version>     
    
</config>